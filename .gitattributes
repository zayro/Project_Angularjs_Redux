# Archivos generados
public/* linguist-generated=true

# Archivos de documentacion
docs/* linguist-documentation

# Excluir folder al clonar
test/ export-ignore

# Archivos analizar
app/* linguist-vendored

# Analizar archivos Sql
#*.sql linguist-generated=true

# ignora porcentaje de codigo html
#index.html linguist-vendored=false

# ignora lenguaje programacion
#*.html linguist-detectable=false

# Crear una extension con un lenguaje especifico
# *.rb linguist-language=Java
